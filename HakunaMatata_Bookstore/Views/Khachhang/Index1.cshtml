@model IEnumerable<HakunaMatata_Bookstore.Models.KHACHHANG>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index1</title>
</head>
<body>
    <div> 
        <table border="1">
            <tr>
                <td>Mã khách hàng</td>
                <td>Tên khách hàng</td>
                <td>Địa chỉ</td>
                <td>SĐT</td>
                <td>Email</td>
            </tr>
                @*Chạy 1 vòng lặp lấy ra các đối tượng từ list model*@
                @foreach(var item in Model) 
                { 
                  <tr>
                      <td>@item.MaKH</td>
                      <td>@item.TenKH</td>
                      <td>@item.Diachi</td>
                      <td>@item.SĐT</td>
                      <td>@item.Email</td>
                  </tr>
                }
            
        </table>
    </div>
</body>
</html>
